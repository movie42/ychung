extends ../layout/layout.pug
include ../mixins/worshipList.pug

block content
    .inform_wrapper
        h1.head_title 예배
        if loggedIn
            .button_wrapper
                a(href="/worship/upload").write_button
                    span.material-icons-outlined add_circle
    .list_wrapper
        ul.list_item
            each item in data
                +worshipBlock({
                    id:item.id,
                    title:item.title,
                    year:item.year,
                    month:item.month,
                    date:item.date
                })

