extends ../layout/layout.pug
include ../mixins/weeklyList.pug

block content
    .inform_wrapper
        h1.head_title 주보
        .button_wrapper
            a(href="/weekly/upload").write_button
                span.material-icons-outlined add_circle
    .list_wrapper
        ul.list_item
            each item in data
                +weeklyBlock({
                    id:item.id,
                    title:item.title,
                    year:item.year,
                    month:item.month,
                    date:item.date
                })

